select * from
	rp_cdkt_final
where
	ngay_dl = to_date($P{ngay_dl}, 'dd/MM/yyyy')
	and (($P{ma_don_vi} = '') is not false
	or $P{ma_don_vi} = ma_don_vi)
	and (($P{capdo_baocao} = '') is not false
	or capdo_baocao = $P{capdo_baocao})
			
			
			
			
			
			
			
			
			
			
			
			
			
			
			
